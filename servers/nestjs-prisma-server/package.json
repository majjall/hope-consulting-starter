{
  "name": "nestjs-prisma-server",
  "version": "0.0.1",
  "description": "NestJS Prisma Project",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "nestjs",
    "nexus",
    "nexus-plugin-prisma",
    "prisma",
    "prisma client",
    "typescript",
    "passport",
    "graphql"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "rimraf dist",
    "build": "npm -s run clean && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",

    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:generate:watch": "npx prisma generate --schema=./prisma/schema.prisma --watch ",
    "prisma:format": "npx prisma format --schema=./prisma/schema.prisma",
    "prisma:db-pull": "npx prisma db pull --schema=./prisma/schema.prisma",
    "prisma:db-push": "npx prisma db pull --schema=./prisma/schema.prisma --skip-generate --force-reset --accept-data-loss",
    "prisma:db-seed": "npx prisma db seed",
    "prisma:migrate-dev": "npx prisma migrate dev --skip-generate",
    "prisma:migrate-dev:prompt": "npx prisma migrate dev --preview-feature --schema=./prisma/schema.prisma --skip-generate --skip-seed --create-only",
    "prisma:migrate-status": "npx prisma migrate status --schema=./prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/apollo": "10.x",
    "@nestjs/common": "9.x",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.x",
    "@nestjs/graphql": "10.x",
    "@nestjs/jwt": "9.x",
    "@nestjs/mapped-types": "^0.3.0",
    "@nestjs/passport": "9.x",
    "@nestjs/platform-express": "9.x",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/throttler": "^3.0.0",
    "@paljs/nexus": "4.0.19",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "4.x",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "express-rate-limit": "^6.5.2",
    "express-session": "^1.17.3",
    "graphql": "15.x",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.1.31",
    "graphql-scalars": "^1.17.0",
    "graphql-shield": "^7.5.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.3.0",
    "graphql-upload": "16.x.x",
    "graphql-ws": "^5.9.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "8.5.1",
    "nestjs-prisma": "^0.18.0",
    "nexus": "1.x",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.5.2",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.x",
    "@nestjs/schematics": "9.x",
    "@nestjs/testing": "9.x",
    "@paljs/cli": "4.0.19",
    "@types/bcrypt": "^5.0.0",
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "15.x",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chance": "^1.1.8",
    "eslint": "8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.x",
    "prisma": "4.x",
    "prisma-dbml-generator": "^0.9.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "10.x",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
